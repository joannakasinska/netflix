  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
