{
  "printWidth": 160,
  "tabWidth": 4,
  "semi": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "trailingComma": "all",
  "importOrderSortSpecifiers": true,
  "importOrder": [
    "api",
    "utils",
    "assets",
    "context",
    "providers",
    "container",
    "pages",
    "components",
    "^[./]",
    "^[./](.*types)",
    "^[./](.*scss)"
  ]
}
